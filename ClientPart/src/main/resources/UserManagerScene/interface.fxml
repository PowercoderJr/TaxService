<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="root" opacity="0.99" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TaxService.SceneControllers.UserManagerController">
   <left>
      <VBox alignment="TOP_CENTER" prefWidth="300.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="createBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="290.0" text="Создать" />
            <Button fx:id="editBtn" disable="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="290.0" text="Изменить" />
            <Button fx:id="toggleBlockedBtn" disable="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="290.0" text="Заблокировать / Разблокировать" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </left>
   <center>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
